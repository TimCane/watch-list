{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "api-client": {
      "projectType": "library",
      "root": "projects/api-client",
      "sourceRoot": "projects/api-client/src",
      "prefix": "api",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/api-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/api-client/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/api-client/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/api-client/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/website",
      "sourceRoot": "projects/website/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/website",
            "index": "projects/website/src/index.html",
            "main": "projects/website/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/website/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/website/src/favicon.ico",
              "projects/website/src/assets"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/lara-dark-teal/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "projects/website/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "projects/website/src/environments/environment.ts",
                  "with": "projects/website/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "website:build:production"
            },
            "development": {
              "browserTarget": "website:build:development"
            }
          },
          "options": {
            "port": 4224
        },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/website/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/website/src/favicon.ico",
              "projects/website/src/assets"
            ],
            "styles": [
              "projects/website/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
